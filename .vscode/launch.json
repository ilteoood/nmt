    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'cli'",
            "cargo": {
                "args": [
                    "build",
                    "--bin=cli",
                    "--package=nmt"
                ],
                "filter": {
                    "name": "cli",
                    "kind": "bin"
                }
            },
            "args": [
                "--project-root-location",
                "/Users/ilteoood/Documents/git/personal/node-red",
                "--entry-point-location",
                "/Users/ilteoood/Documents/git/personal/node-red/packages/node_modules/node-red/red.js"
            ],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in executable 'cli'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--bin=cli",
                    "--package=nmt"
                ],
                "filter": {
                    "name": "cli",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
